{
  "name": "justreadcomics-lambda",
  "description": "Lambdas for Just Read Comics",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "eslint": "^8.55.0",
    "jest": "^29.2.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build:production": "NODE_ENV=production tsc",
    "build": "NODE_ENV=development tsc",
    "start": "",
    "start:dev": ""
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/__tests__/**/*.mjs?(x)",
      "**/?(*.)+(spec|test).mjs?(x)"
    ],
    "moduleFileExtensions": [
      "mjs",
      "js"
    ]
  },
  "dependencies": {
    "@justreadcomics/common": "1.0.0",
    "@types/aws-lambda": "^8.10.130",
    "puppeteer": "^21.5.2"
  }
}
